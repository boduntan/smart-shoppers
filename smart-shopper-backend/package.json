{
  "name": "staples-smart-shopper-backend",
  "version": "1.0.0",
  "description": "AI-powered shopping assistant backend for Staples Canada",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
